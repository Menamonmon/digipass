{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "next lint",
    "relay": "relay-compiler",
    "start": "next start",
    "dev": "next dev",
    "build": "next build",
    "generate": "graphql-codegen --config codegen.yml && get-graphql-schema http://localhost:4000/graphql > ./src/graphql/schema.graphql",
    "init-relay": "npm run relay",
    "init-relay-dev": "npm run relay --watch",
    "predev": "npm run init-relay-dev",
    "prestart": "npm run init-relay"
  },
  "relay": {
    "src": "./src",
    "language": "typescript",
    "schema": "./src/data/schema.graphql",
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "daisyui": "^2.17.0",
    "gapi-script": "^1.2.0",
    "next": "12.1.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-login": "^5.2.2",
    "react-icons": "^4.3.1",
    "react-relay": "^14.0.0",
    "relay-runtime": "^14.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.2",
    "@graphql-codegen/fragment-matcher": "3.3.0",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-graphql-request": "4.5.2",
    "@graphql-codegen/typescript-operations": "2.5.2",
    "@graphql-codegen/typescript-react-apollo": "3.3.2",
    "@types/node": "17.0.35",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/react-relay": "^13.0.2",
    "autoprefixer": "^10.4.7",
    "babel-plugin-relay": "^14.0.0",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^16.5.0",
    "postcss": "^8.4.14",
    "relay-compiler": "^14.0.0",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  }
}
